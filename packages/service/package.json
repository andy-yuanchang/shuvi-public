{
  "name": "@shuvi/service",
  "version": "1.0.0-rc.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/shuvi"
  },
  "author": "liximomo",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "lib",
    "esm"
  ],
  "exports": {
    ".": {
      "default": "./lib/index.js"
    },
    "./project": {
      "default": "./lib/project/index.js"
    },
    "./project/file-utils": {
      "default": "./lib/project/file-utils/index.js"
    },
    "./project/*": {
      "default": "./lib/project/*.js"
    },
    "./core": {
      "default": "./lib/core/index.js"
    },
    "./core/config": {
      "default": "./lib/core/config/index.js"
    },
    "./core/*": {
      "default": "./lib/core/*.js"
    },
    "./bundler": {
      "default": "./lib/bundler/index.js"
    },
    "./bundler/config": {
      "default": "./lib/bundler/config.js"
    },
    "./bundler/bundler": {
      "default": "./lib/bundler/bundler.js"
    },
    "./bundler/*": {
      "default:": "./lib/bundler/*.js"
    },
    "./server": {
      "default": "./lib/server/index.js"
    },
    "./server/middlewares/dev": {
      "default": "./lib/server/middlewares/dev/index.js"
    },
    "./server/*": {
      "default": "./lib/server/*.js"
    },
    "./resources": {
      "default": "./lib/resources.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.build.json -m commonjs -w",
    "prebuild": "rimraf esm",
    "build": "tsc -p tsconfig.build.json -m commonjs"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@babel/code-frame": "7.14.5",
    "@babel/generator": "7.14.5",
    "@babel/parser": "7.14.7",
    "@babel/traverse": "7.14.7",
    "@shuvi/hook": "1.0.0-rc.1",
    "@shuvi/router": "1.0.0-rc.1",
    "@shuvi/runtime": "1.0.0-rc.1",
    "@shuvi/shared": "1.0.0-rc.1",
    "@shuvi/toolpack": "1.0.0-rc.1",
    "@shuvi/utils": "1.0.0-rc.1",
    "@vue/reactivity": "3.1.4",
    "commander": "5.1.0",
    "cross-spawn": "7.0.3",
    "detect-port": "1.3.0",
    "fs-extra": "9.0.1",
    "http-proxy-middleware": "1.0.6",
    "launch-editor": "2.2.1",
    "rimraf": "3.0.2",
    "send": "0.17.1",
    "ws": "8.2.3"
  },
  "devDependencies": {
    "@babel/types": "7.12.11",
    "@types/babel__code-frame": "^7.0.3",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.14.2",
    "@types/cross-spawn": "6.0.1",
    "@types/detect-port": "1.3.0",
    "@types/ejs": "3.0.4",
    "@types/ws": "8.2.0"
  }
}
