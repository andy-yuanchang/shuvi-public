{
  "name": "@shuvi/platform-shared",
  "version": "1.0.0-rc.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/core"
  },
  "type": "module",
  "author": "liximomo",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "esm",
    "template",
    "shuvi-type-extensions-node.d.js",
    "shuvi-type-extensions-runtime.d.ts"
  ],
  "exports": {
    "./shared": {
      "default": "./esm/shared/index.js"
    },
    "./shared/*": {
      "default": "./esm/shared/*.js"
    },
    "./shared/loader": {
      "default": "./esm/shared/loader/index.js"
    },
    "./shared/store": {
      "default": "./esm/shared/store/index.js"
    },
    "./node": {
      "default": "./esm/node/index.js"
    },
    "./node/*": {
      "default": "./esm/node/*.js"
    },
    "./shuvi-app": {
      "default": "./esm/shuvi-app/index.js"
    },
    "./shuvi-app/*": {
      "default": "./esm/shuvi-app/*.js"
    },
    "./template/*": "./template/*",
    "./shuvi-type-extensions-node": {
      "types": "./shuvi-type-extensions-node.d.js",
      "default": "./shuvi-type-extensions-node.mjs"
    },
    "./shuvi-type-extensions-runtime": "./shuvi-type-extensions-runtime.d.ts"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.build.json -w",
    "prebuild": "rimraf esm",
    "build": "tsc -p tsconfig.build.json"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@shuvi/hook": "1.0.0-rc.1",
    "@shuvi/redox": "0.0.3",
    "@shuvi/router": "1.0.0-rc.1",
    "@shuvi/runtime": "1.0.0-rc.1",
    "@shuvi/service": "1.0.0-rc.1",
    "@shuvi/shared": "1.0.0-rc.1",
    "@shuvi/toolpack": "1.0.0-rc.1",
    "@shuvi/utils": "1.0.0-rc.1",
    "redux": "4.1.2"
  },
  "peerDependencies": {
    "@shuvi/service": "1.0.0-rc.1"
  },
  "devDependencies": {
    "@types/minimatch": "3.0.5"
  }
}
